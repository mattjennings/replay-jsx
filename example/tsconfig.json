{
  "compilerOptions": {
    "target": "es6",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve"
  },
  "include": ["src"]
}
